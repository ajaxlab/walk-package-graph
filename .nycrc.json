{
  "check-coverage": true,
  "per-file": true,
  "lines": 99,
  "statements": 99,
  "functions": 99,
  "branches": 99,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "extension": [
    ".ts"
  ],
  "cache": true,
  "all": true,
  "temp-dir": "./.coverage/tmp",
  "report-dir": "./.coverage"
}
